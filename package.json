{
  "name": "mixpanel-import",
  "version": "3.1.2",
  "description": "stream and import data to mixpanel from node.js",
  "main": "index.js",
  "types": "index.d.ts",
  "keywords": [
    "mixpanel",
    "stream",
    "analytics",
    "tracking",
    "server",
    "CLI"
  ],
  "scripts": {
    "start": "node index.js --ui",
    "dev": "npm run prune && nodemon --inspect scratch.js --ignore ./testData --ignore ./mixpanel-exports --ignore ./logs",
    "post": "npm publish",
    "generate": "NUMEVENTS=3000 SINCEDAYS=7 node ./components/fakeData.js",
    "benchmark": "node ./benchmarks/benchmark.js",
    "prune": "rm -rf ./logs/* && rm -rf ./mixpanel-exports/* && rm -rf ./tmp/* && rm -rf ./benchmarks/results/*",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "coverage": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js --coverage && open ./tests/coverage/lcov-report/index.html",
    "cli:test": "node index.js ./testData/moarEvents.json --fixData",
    "cli:help": "node index.js --help",
    "typecheck": "npx tsc",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "validate": "npm run typecheck && npm run lint",
    "deploy": "gcloud builds submit --config cloudbuild.yaml --region us-central1",
    "ui:dev": "nodemon --inspect ui/server.js --ignore ./tmp --ignore ./logs"
  },
  "bin": {
    "mixpanel-import": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ak--47/mixpanel-import"
  },
  "author": "AK",
  "license": "ISC",
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.0.0"
  },
  "bugs": {
    "url": "https://github.com/ak--47/mixpanel-import/issues"
  },
  "homepage": "https://github.com/ak--47/mixpanel-import#readme",
  "files": [
    "index.js",
    "index.d.ts",
    "components/**/*.js",
    "vendor/**/*.js",
    "ui/**/*",
    "README.md",
    "LICENSE",
    "CLAUDE.md",
    "package.json"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@google-cloud/pino-logging-gcp-config": "^1.1.0",
    "@google-cloud/storage": "^7.16.0",
    "ak-fetch": "^2.0.0",
    "ak-tools": "^1.1.0",
    "chance": "^1.1.7",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.7",
    "dotenv": "^10.0.0",
    "express": "^5.1.0",
    "got": "^11.8.6",
    "hyparquet": "^1.17.1",
    "json-stable-stringify": "^1.0.2",
    "md5": "^2.3.0",
    "multer": "^2.0.2",
    "multistream": "^4.1.0",
    "murmurhash": "^2.0.1",
    "node-gzip": "^1.1.2",
    "papaparse": "^5.3.2",
    "parallel-transform": "^1.2.0",
    "pino": "^9.9.2",
    "stream-json": "^1.8.0",
    "underscore": "^1.13.2",
    "undici": "^7.15.0",
    "ws": "^8.18.0",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/multistream": "^4.1.0",
    "eslint": "^8.26.0",
    "jest": "^29.3.1",
    "jest-extended": "^4.0.1",
    "make-mp-data": "^1.5.54",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "typescript": "^5.2.2"
  }
}
