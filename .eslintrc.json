{
	"env": {
		"browser": false,
		"node": true,
		"commonjs": true,
		"es2021": true
	},
	"extends": "eslint:recommended",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest"
	},
	"rules": {
		"no-prototype-builtins": "off",
		"no-debugger": "off",
		"no-useless-escape" : "off",
		"no-unused-vars": [
			"warn",
			{
				"argsIgnorePattern": "^_",
				"varsIgnorePattern": "^_"
			}
		]
	},
	"globals": {
		"l": true
	},
	"ignorePatterns": [
		"benchmarks/*"
	]
}